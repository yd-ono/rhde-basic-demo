---
kind: OperatorGroup
apiVersion: operators.coreos.com/v1
metadata:
  name: skupper-operator
  namespace: skupper
spec:
  targetNamespaces:
{% for student_number in range(1, student_total + 1) %}
    - "student{{ student_number }}-workshop-{{ workshop_type | regex_replace('_', '-') }}"
{% endfor %}